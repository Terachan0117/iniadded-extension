{
    "name": "INIADded Extension",
    "short_name": "INIADded",
    "description": "Chrome extension to add various useful functions to \"INIAD MOOCs\"",
    "version": "8.1.3",
    "icons": {
        "16": "img/icon16.png",
        "48": "img/icon48.png",
        "128": "img/icon128.png"
    },
    "browser_action": {
        "default_icon": "img/icon16.png",
        "default_title": "INIADded"
    },
    "content_scripts": [{
        "matches": [
            "https://moocs.iniad.org/*",
            "https://g-sys.toyo.ac.jp/*",
            "https://docs.google.com/presentation/*"
        ],
        "css": [
            "css/style.css"
        ],
        "js": [
            "lib/jquery.min.js",
            "lib/jquery-ui.min.js",
            "js/modal-window.js",
            "js/new-lectures.js",
            "js/my-courses.js",
            "js/sticky-note.js",
            "js/settings.js",
            "js/problem-reminder.js",
            "js/main.js",    
            "js/change-color.js",
            "js/set-wallpaper.js",
            "js/fixed-header.js",
            "js/change-grid.js",
            "js/count-character.js",
            "js/back-to-top.js",
            "js/python-repl.js",
            "lib/FileSaver.min.js",
            "lib/jszip.min.js",
            "js/capture-slides.js"
        ]
    }],
    "background": {
        "scripts": [
            "js/background.js"
        ]
    },
    "manifest_version": 2
}
