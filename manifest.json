{
    "name": "INIADded Extension",
    "short_name": "INIADded",
    "description": "Chrome extension to add various useful functions to \"INIAD MOOCs\"",
    "version": "8.0.0",
    "icons": {
        "16": "img/icon16.png",
        "48": "img/icon48.png",
        "128": "img/icon128.png"
    },
    "browser_action": {
        "default_icon": "img/icon16.png",
        "default_title": "INIADded"
    },
    "content_scripts": [{
        "matches": [
            "https://moocs.iniad.org/*",
            "https://g-sys.toyo.ac.jp/*",
            "https://docs.google.com/presentation/*"
        ],
        "js": [
            "lib/jquery.min.js",
            "js/main.js",
            "js/pythonRepl.js",
            "js/countCharacter.js",
            "js/changeGrid.js",
            "js/changeColor.js",
            "js/setWallpaper.js"
        ]
    }],
    "background": {
        "scripts": [
            "js/background.js"
        ]
    },
    "manifest_version": 2
}
